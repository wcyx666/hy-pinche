/* pages/travel/travel.wxss */
.travel {
  width: 100%;
  height: 100%;
  position: fixed;
  background: #f3f3f3;
}
.radio-group {
  width: 100%;
  overflow: hidden;
  height: 80rpx;
  background: #5596e5;
  color: #fff;
  position: fixed;
  top: 0;
  z-index: 9;
}
.radio-group label {
  float: left;
  display: block;
  width: 25%;
  text-align: center;
  line-height: 80rpx;
  font-size: 16px;
  color: #e6e6e6;
}
.radio-group label radio {
  display: none;
}
.list {
  padding: 0 30rpx;
  overflow: hidden;
  background: #fff;
}
.list_left {
  width: 200rpx;
  height: 150rpx;
  float: left;
  margin-right: 30rpx;
  position: relative;
}
.list_left image {
  width: 100%;
  height: 100%;
}
.list_right {
  width: 50%;
  float: left;
}
.server_title {
  font-size: 16px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.server_title text {
  font-size: 14px;
  color: #333;
}
.server_text {
  font-size: 14px;
  padding-top: 10rpx;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.server_text image {
  width:50rpx;
  height:36rpx;
}
.top_btn {
  overflow: hidden;
  padding: 20rpx 0;
  background: #5596e5;
  color: #fff;
}
.top_btn .icon {
  width: 25%;
  float: left;
  text-align: center;
  font-size: 14px;
}
.release {
  position: fixed;
  bottom: 100rpx;
  right: 30rpx;
  width: 120rpx;
  height: 120rpx;
  text-align: center;
  line-height: 120rpx;
  color: #fff;
  background: #333;
  border-radius: 120rpx;
  opacity: 0.5;
}
.ash {
  width: 100%;
  height: 15rpx;
  background: #e6e6e6;
}
.icon_phone {
  float: right;
  width: 100rpx;
  height: 100rpx;
  margin-top: 32rpx;
}
.icon_phone image {
  width: 100%;
  height: 100%;
}
.null {
  text-align: center;
  margin-top: 100rpx;
}
.null image {
  width: 130rpx;
  height: 120rpx;
}
.null text {
  display: block;
  font-size: 14px;
  color: #e6e6e6;
}
.active {
  font-size: 18px !important;
  color: #fff !important;
  border-bottom: 1px solid #fff;
  margin-top:-1px;
}

.max {
  font-size: 12px;
  width: 40rpx;
  height: 40rpx;
  background: red;
  border-radius: 40rpx;
  position: absolute;
  text-align: center;
  line-height: 40rpx;
  color: #fff;
  top: 0;
  right: 0;
}
.tis {
  color: red;
  margin-top: 30rpx;
  font-size: 12px;
  padding: 0 30rpx;
}
.info_num {
  padding: 20rpx 0;
  font-size: 14px;
  border-bottom: 1px solid #f3f3f3;
}
.info_content {
  overflow: hidden;
  padding: 20rpx 0;
}
.butPad {
  padding: 30rpx;
}
.default {
  width: 100%;
  background-color:#5596e5 !important;
  color:#fff;
  text-align:center;
  height:80rpx;
  line-height:80rpx;
  font-size: 14px;
}