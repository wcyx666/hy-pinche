/* pages/travel/travel.wxss */
.travel {
  width: 100%;
  height: 100%;
  position: fixed;
  background: #f3f3f3;
}
.radio-group {
  width: 100%;
  overflow: hidden;
  height: 80rpx;
  background: #5596e5;
  color: #fff;
  position: fixed;
  top: 0;
  z-index: 9;
}
.radio-group label {
  float: left;
  display: block;
  width: 25%;
  text-align: center;
  line-height: 80rpx;
  font-size: 16px;
  color: #e6e6e6;
}
.radio-group label radio {
  display: none;
}
.list {
  padding: 0 30rpx;
  overflow: hidden;
  background: #fff;
  font-size: 14px;
}
.list_top {
  width: 100%;
  overflow: hidden;
}
.list_top_left {
  width: 70%;
  float: left;
}
.list_top_left_localc {
  margin-bottom:22rpx;
}
.list_top_left_localc text {
  background: #85dc84;
  width: 45rpx;
  height: 45rpx;
  float: left;
  text-align: center;
  line-height: 45rpx;
  border-radius: 2px;
  color: #fff;
  display: block;
  margin-right: 10rpx;
}
.list_top_left_localc text.zong {
  background: #ff6669;
}
.list_top_right {
  width: 28%;
  height: 30px;
  border: 1px solid #85dc84;
  text-align: center;
  line-height: 30px;
  float: right;
  margin-top: 12px;
  font-size: 12px;
  color: #85dc84;
  border-radius: 5px;
}
.list_bottom_left {
  width: 70%;
  float: left;
  color: #888;
}
.list_bottom_prot {
  font-size: 12px;
  color: #85dc84;
  margin-bottom: 10rpx;
}
.list_bottom_get view {
  margin-bottom: 10rpx;
}
.list_bottom_info {
  overflow: hidden;
}
.list_bottom_info text {
  display: block;
  width: 80rpx;
  height: 45rpx;
  background: #f3a42b;
  color: #fff;
  text-align: center;
  border-radius: 3px;
  float: left;
  margin-right: 10rpx;
}
.list_bottom {
  height: 98px;
  overflow: hidden;
  position: relative;
}
.list_bottom_right {
  width:28%;
  height:30px;
  border:1px solid #85dc84;
  text-align:center;
  line-height:30px;
  float:right;
  font-size:12px;
  color:#85dc84;
  border-radius:5px;
  position: absolute;
  bottom: 12px;
  right: 0;
}
.list_bottom_right image {
  width: 130rpx;
  height: 130rpx; 
  margin-top: 38rpx;
}
.null {
  text-align: center;
  margin-top: 100rpx;
}
.null image {
  width: 130rpx;
  height: 120rpx;
}
.null text {
  display: block;
  font-size: 14px;
  color: #e6e6e6;
}
.active {
  font-size: 18px !important;
  color: #fff !important;
  border-bottom: 1px solid #fff;
  margin-top:-1px;
}

.max {
  font-size: 12px;
  width: 40rpx;
  height: 40rpx;
  background: red;
  border-radius: 40rpx;
  position: absolute;
  text-align: center;
  line-height: 40rpx;
  color: #fff;
  top: 0;
  right: 0;
}
.tis {
  color: red;
  margin-top: 30rpx;
  font-size: 12px;
  padding: 0 30rpx;
}
.info_num {
  padding: 20rpx 0;
  font-size: 14px;
  border-bottom: 1px solid #f3f3f3;
}
.info_content {
  overflow: hidden;
  padding: 20rpx 0;
}
.butPad {
  padding: 30rpx;
}
.default {
  width: 100%;
  background-color:#5596e5 !important;
  color:#fff;
  text-align:center;
  height:80rpx;
  line-height:80rpx;
  font-size: 14px;
}
.serverUid {
  padding: 20rpx 0;
}
.serverUid text {
  color: #ff6669;
  margin-left: 20rpx;
}