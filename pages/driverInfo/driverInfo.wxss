/* pages/travel/travel.wxss */
.travel {
  width: 100%;
}
.list {
  padding: 30rpx;
  border-bottom: 5px solid #f3f3f3;
  overflow: hidden;
  font-size: 14px;
}
.list:last-child {
  border-bottom: none;
}
.list_top {
  width: 100%;
  overflow: hidden;
}
.list_top_left {
  width: 70%;
  float: left;
}
.list_top_left_localc {
  margin-bottom:22rpx;
}
.list_top_left_localc text {
  background: #85dc84;
  width: 45rpx;
  height: 45rpx;
  float: left;
  text-align: center;
  line-height: 45rpx;
  border-radius: 2px;
  color: #fff;
  display: block;
  margin-right: 10rpx;
}
.list_top_left_localc text.zong {
  background: #ff6669;
}
.list_top_right {
  width: 28%;
  height: 30px;
  border: 1px solid #85dc84;
  text-align: center;
  line-height: 30px;
  float: right;
  margin-top: 12px;
  font-size: 12px;
  color: #85dc84;
  border-radius: 5px;
}
.list_bottom_left {
  width: 70%;
  float: left;
  color: #888;
}
.list_bottom_prot {
  font-size: 12px;
  color: #85dc84;
  margin-bottom: 10rpx;
}
.list_bottom_get view {
  margin-bottom: 10rpx;
}
.list_bottom_info {
  overflow: hidden;
}
.list_bottom_info text {
  display: block;
  width: 80rpx;
  height: 45rpx;
  background: #f3a42b;
  color: #fff;
  text-align: center;
  border-radius: 3px;
  float: left;
  margin-right: 10rpx;
}
.list_bottom {
  height: 98px;
  overflow: hidden;
  position: relative;
}
.list_bottom_right {
  width:28%;
  height:30px;
  border:1px solid #85dc84;
  text-align:center;
  line-height:30px;
  float:right;
  font-size:12px;
  color:#85dc84;
  border-radius:5px;
  position: absolute;
  bottom: 12px;
  right: 0;
}
.list_bottom_right image {
  width: 130rpx;
  height: 130rpx; 
  margin-top: 38rpx;
}
.footer_bottom {
  text-align: center;
  font-size: 12px;
  color: #888;
  padding-bottom: 10rpx;
}

