/* pages/release/release.wxss */
.releasse {
  width: 100%;
  height: 100%;
  background: #f5f5f5;
  position: fixed;
}
.releasse_form_content {
  background: #fff;
}
.releasse_form_title {
  font-size: 14px;
  color: #e6e6e6;
  padding: 20rpx;
}
.releasse_prompt {
  width: 100%;
  font-size: 12px;
  background: #5596e5;
  padding: 6rpx;
  color: #fff;
  border-radius: 10rpx;
  margin: 0 auto;
}
.releasse_form {
  widows: 100%;
}
.releasse_form textarea {
  font-size: 14px;
  float: left;
  width: 70%;
  background: #e5e5e5;
  padding: 15rpx;
}
.releasse_form_map input {
  font-size: 14px;
  float: left;
  width: 70%;
}
.releasse_form_map {
  width: 100%;
  padding: 20rpx;
  border-bottom: 1px solid #ebebeb;
  overflow: hidden;
}
.releasse_form_name {
  float: left;
  font-size: 14px;
  width: 23%;
  margin-top: 6rpx;
}
.releasse_form_icon {
  width:80%;
  float: left;
}
.releasse_form_icon .label {
  float: left;
  width: 20%;
  font-size: 14px;
  text-align: center;
  border:1px solid #5596e5;
  margin-right: 20rpx;
}
.default {
  background: #5596e5;
  color: #fff;
  text-align: center;
  width:96%;
  height: 80rpx;
  line-height: 80rpx;
  margin: 0 auto;
}
.reg-agr {
  font-size: 12px;
  color: #333;
  padding: 20rpx;
}
.reg-agr text {
  color:#5596e5; 
}