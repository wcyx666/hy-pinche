/* pages/travel/travel.wxss */
.travel {
  width: 100%;
}
.list {
  padding: 30rpx;
  border-bottom: 1px solid #f3f3f3;
  overflow: hidden;
}
.list_left {
  width: 200rpx;
  height: 150rpx;
  float: left;
  margin-right: 30rpx;
}
.list_left image {
  width: 100%;
  height: 100%;
}
.list_right {
  width: 50%;
  float: left;
}
.server_title {
  font-size: 16px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.server_text {
  font-size: 14px;
  padding-top: 10rpx;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.top_btn {
  overflow: hidden;
  padding: 20rpx 0;
  background: #5596e5;
  color: #fff;
}
.top_btn .icon {
  width: 25%;
  float: left;
  text-align: center;
  font-size: 14px;
}
.release {
  position: fixed;
  bottom: 100rpx;
  right: 30rpx;
  width: 120rpx;
  height: 120rpx;
  text-align: center;
  line-height: 120rpx;
  color: #fff;
  background: #333;
  border-radius: 120rpx;
  opacity: 0.5;
}
.ash {
  width: 100%;
  height: 15rpx;
  background: #e6e6e6;
}
.icon_phone {
  float: right;
  width: 100rpx;
  height: 100rpx;
  margin-top: 32rpx;
}
.icon_phone image {
  width: 100%;
  height: 100%;
}